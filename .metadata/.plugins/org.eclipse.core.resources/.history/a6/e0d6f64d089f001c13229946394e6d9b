package lab6;
import java.util.*;
class PersonApp {
	
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	System.out.println("enter first name");
	try {
		String a=sc.nextLine();
		String b=sc.nextLine();
		check(a,b);
	}
	catch(Nameblankexception e)
	{
		System.out.println(e.getMessage());
	}
}

public static void check(String fname,String lname)throws Nameblankexception
{
	if(fname.isEmpty())
	{
		throw new Nameblankexception("enetr first name first it is blank");
	}
	if(lname.isEmpty())throw new Nameblankexception("enetr last name first it is blank");
	else {
		Person obj=new Person("kanchan","rawat",'f');
		System.out.println(obj);}
}
}
